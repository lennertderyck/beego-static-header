nt-size: 1.2rem;\n                \n                a:not(.btn) {@extend .unlinkify;}\n            }\n        }\n    }\n    \n    &__append {\n        grid-column: 5 / 6;\n        grid-row: 2 / 2;\n        \n        h2 {\n            margin: 0px !important;\n        }\n    }\n    \n    &.header--scrolled {\n        .header__wrapper {\n            grid-template-columns: auto auto auto auto auto auto;\n        }\n        \n        .header__logo {\n            grid-column: 1 / 2;\n            grid-row: 1 / 1;\n        }\n        .header__nav {\n            grid-column: 2 / 4;\n            grid-row: 1 / 1;\n        }\n        .header__append {\n            grid-column: 4 / 6;\n            grid-row: 1 / 1;\n        }\n    }\n} */\n.header {\n    position: fixed;\n    top: 0px;\n    left: 0px; right: 0px;\n    background-color: #001a3b;\n    background-color: white;\n    // background-color: rgb(219, 219, 219);\n    border-bottom: 2px solid lightgray;\n    // box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.185);\n    \n    *:not(.btn) {color: #001a3b};\n    \n    &__wrapper {\n        padding: 2.4rem 2rem 1.8rem 2rem;\n        display: flex;\n        flex-direction: column;\n    }\n    \n    &__top {\n        margin-bottom: 1rem;\n    }\n    \n    &__bottom {\n        display: grid;\n        grid-template-columns: 0px auto auto;\n        // justify-content: space-between;\n        \n        .header__logo {\n            display: flex;\n            grid-column: 1 / 1;\n            justify-content: center;\n            align-items: center;\n            max-width: 0vw;\n            overflow: hidden;\n            \n            // transform: scale(0);\n            \n            img {\n                height: 70px;\n            }\n        }\n        \n        .header__nav {   \n            grid-column: 2 / 4;  \n               \n            nav ul {\n                display: flex;\n                align-items: center;\n                flex-wrap: wrap;\n                list-style: none;\