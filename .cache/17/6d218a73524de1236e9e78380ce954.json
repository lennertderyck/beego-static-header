n: relative;\n          display: block;\n          padding: 0.6rem 0.8rem;\n          border-radius: 6px;\n          font-size: 1.2rem;\n\n          a {\n            display: flex;\n            align-items: center;\n\n            .feather {\n              margin-left: 8px;\n              width: 20px;\n              transform: rotate(0deg);\n            }\n          }\n\n          &:hover {\n            background-color: var(--header-list-bg);\n\n            a .feather {\n              transform: rotate(-180deg);\n              //   transform: scaleY(-1);\n            }\n\n            .nav__item--embed {\n              max-height: 100vh;\n              background-color: var(--header-list-bg);\n            }\n          }\n\n          &.effect--no-hover {\n            background-color: transparent !important;\n          }\n\n          a:not(.btn) {\n            @extend .unlinkify;\n          }\n        }\n      }\n    }\n\n    .header__append {\n      display: flex;\n      justify-content: flex-end;\n      align-items: center;\n      grid-column: 4 / 6;\n\n      h2 {\n        margin: 0px !important;\n      }\n    }\n  }\n\n  &__logo {\n    display: flex;\n    justify-content: center;\n    transform: scale(1);\n    max-height: 100vh;\n\n    img {\n      // width: 100%;\n      height: 100px;\n      height: var(--scroll-process-px);\n      object-fit: contain;\n    }\n  }\n\n  &.header--scrolled {\n    .header__wrapper {\n      padding: 1rem 2rem 1rem 2rem;\n    }\n    .header__top {\n      margin-bottom: 0;\n    }\n    .header__top .header__logo {\n      grid-column: 1 / 2;\n      max-height: 0vh;\n      transform: scale(0);\n    }\n\n    .header__bottom {\n      grid-template-columns: auto auto auto;\n    }\n\n    .header__bottom .header__logo {\n      max-width: 100vw;\n      // animation: logoIn .3s forwards;\n\n      img {\n        transform: scale(1);\n      }\n    }\n  }\n}\n\n.nav {\n  &__item--has-embed {\n    border-radius: 6px 6px 0px 0px !important;\n  }\n\n  