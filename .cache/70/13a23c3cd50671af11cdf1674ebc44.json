       \n                li {\n                    display: block;\n                    padding: 0 .8rem;\n                    font-size: 1.2rem;\n                    \n                    a:not(.btn) {@extend .unlinkify;}\n                }\n            }\n        }\n        \n        .header__append {   \n            display: flex;\n            justify-content: flex-end;\n            align-items: center;\n            grid-column: 4 / 6; \n                 \n            h2 {\n                margin: 0px !important;\n            }\n        }\n        \n    }\n    \n    &__logo { \n        display: flex;\n        justify-content: center;\n        transform: scale(1);\n        max-height: 100vh;\n               \n        img {\n            // width: 100%;\n            height: 90px;\n            object-fit: contain;\n        }\n    }\n    \n    \n    &.header--scrolled {\n        .header__wrapper {\n            padding: 1rem 2rem 1rem 2rem;\n        }\n        .header__top {margin-bottom: 0;}\n        .header__top .header__logo {\n            grid-column: 1 / 2;\n            max-height: 0vh;\n            transform: scale(0);\n        }\n        \n        .header__bottom {\n            grid-template-columns: auto auto auto;\n        }\n        \n        .header__bottom .header__logo {\n            max-width: 100vw;\n            // animation: logoIn .3s forwards;\n            \n            img {\n                transform: scale(1);\n            }\n        }\n    }\n}\n\n@keyframes logoIn {\n    0% {\n        display: none;\n        max-width: 0vw;\n    }\n    \n    50% {\n        display: block;\n        max-width: 0vw;\n    }\n    100% {\n        display: block;\n        max-width: 100vw;\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"c5e6ed22dc9631157fd09be6ff239e20","cacheData":{"env":{}}}